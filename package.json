{
  "name": "tabata-timer",
  "version": "1.0.0",
  "description": "Tabata timer",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "copy-mp3": "cp -vR ./src/mp3 ./dist",
    "copy-img": "cp -vR ./src/img ./dist",
    "copy-server": "cp -vR ./src/server/ ./dist",
    "init": "npm run clean-dist && npm run copy-mp3 && npm run copy-img && npm run copy-server",
    "dev": "npm run init && parcel src/index.html",
    "build": "parcel build src/index.html",
    "deploy": "npm run init && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akyc/tabata-timer.git"
  },
  "keywords": [
    "react",
    "tabata-timer.ru"
  ],
  "author": "Andrew Zyuzin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akyc/tabata-timer/issues"
  },
  "homepage": "https://github.com/akyc/tabata-timer#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "2.8.3",
    "prettier": "3.1.1",
    "process": "0.11.10"
  },
  "dependencies": {
    "bootstrap": "5.3.0-alpha3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.1",
    "swiper": "^8.4.7"
  }
}
